.SH "PAGE METHODS"
.IX Header "PAGE METHODS"
.ie n .IP "$page = $pdf\->\fIpage()\fR" 4
.el .IP "\f(CW$page\fR = \f(CW$pdf\fR\->\fIpage()\fR" 4
